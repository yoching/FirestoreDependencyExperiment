# platform :ios, '9.0'

target 'FirestoreDependencyExperiment' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!

  # Pods for FirestoreDependencyExperiment
  pod 'Firebase/Core'
  pod 'Firebase/Firestore'

end

